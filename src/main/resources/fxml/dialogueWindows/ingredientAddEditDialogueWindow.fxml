<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ua.in.dris4ecoder.controllers.fxControllers.dialogueWindowControllers.IngredientAddEditDialogueWindowController">
   <children>
      <HBox>
         <children>
            <TextField fx:id="textFieldIngredientName" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <ComboBox fx:id="comboBoxUnits" prefWidth="80.0">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#saveAction" prefWidth="100.0" text="Ok">
               <HBox.margin>
                  <Insets bottom="10.0" left="50.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button cancelButton="true" mnemonicParsing="false" onAction="#closeAction" prefWidth="100.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</VBox>
